extend type Query{
    findAllAppVersion: [AppVersion]
    findAppVersionByEPlatform(platform: String): AppVersion
}
extend type  Mutation{
                    addAppVersion       (name: String): AppVersion
    deleteAppVersion(platform: String): Boolean
                editAppVersion(id: ID
                    name: String): Role
}

type AppVersion{
    platform: String
    createdAt: String
    updatedAt: String
    latest: String
    min: String
}
