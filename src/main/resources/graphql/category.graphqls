extend type Query{
    findAllCategory: [Category]
    findCategoryById(id: ID): Category
}
extend type  Mutation{
    addCategory(isActive:Boolean
        name: String): Category
    deleteCategory(id: ID):Boolean
    editCategory(id: ID
        isActive:Boolean
        name: String): Category
}

type Category{
    id:ID
    createdAt: String
    updatedAt: String
    isActive:Boolean
    name: String
}
