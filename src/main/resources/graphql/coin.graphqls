extend type Query{
    findAllCoin: [Coin]
    findCoinById(id: ID): Coin
}
extend type  Mutation{
    createCoin(coinInput: CoinInput): Coin
    deleteCoinById(id: ID):Boolean
    updateCoin(id:ID
        coinInput: CoinInput): Coin
}

type Coin{
    id:ID
    createdAt: String
    updatedAt: String
    coin: Float
    won: Float
}

input CoinInput{
    coin: Float
    won: Float
}
