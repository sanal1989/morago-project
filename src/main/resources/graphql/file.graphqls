extend type Query{
    findAllFile: [File]
    findFileById(id: ID): File
}
extend type  Mutation{
    createFile(fileInput: FileInput): File
    deleteFileById(id: ID):Boolean
    updateFile(id: ID
        fileInput: FileInput): File
}

type File{
    id:ID
    createdAt: String
    updatedAt: String
    originalTitle: String
    path: String
    fileType: String
    user: User
}

input FileInput{
    originalTitle: String
    path: String
    fileType: String
    user: Int
}