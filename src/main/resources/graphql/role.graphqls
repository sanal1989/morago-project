extend type Query{
    findAllRole: [Role]
    findRoleById(id: ID): Role
}
extend type  Mutation{
    createRole(roleInput:RoleInput): Role
    deleteRoleById(id: ID): Boolean
    updateRole(id: ID
        roleInput:RoleInput): Role
}

type Role{
    id:ID
    name: ERole
    createdAt: String
    updatedAt: String
}

input RoleInput{
    name: ERole
}
enum ERole{
    ADMIN,
    USER,
    TRANSLATOR
}