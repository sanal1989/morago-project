type Query{
    test:String
}


type Mutation{
    login(phone: String,
        password: String): RefreshTokenResponse
    refreshToken(refreshToken: String): RefreshTokenResponse
    fileUpload(file: Upload!): File!
    multiFileUpload(files: [Upload!]): [File!]!
}

type RefreshTokenResponse{
    accessToken: String
    refreshToken: String
}


